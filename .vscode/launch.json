{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Libtorch Infer Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/backends/libtorch/test/LibtorchInferTest",
            "environment": [{ "name": "config", "value": "Debug" }],
            "cwd": "${workspaceFolder}/build/backends/libtorch/test",
        },
        {
            "name": "OpenCV DNN Infer Test",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/backends/opencv-dnn/test/OCVDNNInferTest",
            "environment": [{ "name": "config", "value": "Debug" }],
            "cwd": "${workspaceFolder}/build/backends/opencv-dnn/test",
        }        
          
    ]
}